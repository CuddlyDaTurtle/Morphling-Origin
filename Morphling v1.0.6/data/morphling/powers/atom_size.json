{
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
        "type": "origins:and",
        "actions": [
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:width 0.025",
                "permission_level": 4
            },
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:height 0.025",
                "permission_level": 4
            },
            {
                "type": "origins:execute_command",
                "command": "scale set pehkui:health 0.1",
                "permission_level": 4
            }
        ]
    },
    "condition": {
        "type": "origins:resource",
        "resource": "morphling:size_resource",
        "comparison": "==",
        "compare_to": 3
    },
    "name": "Pixelated",
    "description": "While Atomized, you are the size of a pixel."
}